{
  "name": "tanstack-query-json-api",
  "version": "1.0.1",
  "description": "Helpers for TanStack Query to work with JSON:API",
  "type": "module",
  "author": "Ben Scholzen 'DASPRiD'",
  "license": "BSD-3-Clause",
  "keywords": [
    "tanstack",
